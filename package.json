{
  "name": "terminal-card",
  "version": "0.0.1",
  "scripts": {
    "start": "NODE_PATH=$NODE_PATH:./shared node --harmony .",
    "dev": "npm run start & webpack-dev-server --progress --color",
    "build": "NODE_ENV=production webpack --progress --color -p --config webpack.prod.config.js",
    "test": "jest --coverage",
    "devDebug": "NODE_PATH=$NODE_PATH:/shared node debug --harmony .",
    "debug": "node debug --harmony ./node_modules/jest/bin/jest.js --coverage --runInBand"
  },
  "jest": {
    "coverageDirectory": "coverage/",
    "rootDir": "./",
    "coverageReporters": [
      "html",
      "text"
    ],
    "verbose": true
  },
  "description": "terminal based suits card game",
  "dependencies": {
    "axios": "^0.7.0",
    "express": "^4.9.8",
    "history": "^1.9.1",
    "immutable": "^3.7.5",
    "jade": "*",
    "lodash": "^3.10.1",
    "object-assign": "^4.0.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-draggable": "^2.2.3",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.0",
    "react-router": "1.0.0-rc3",
    "redux": "^3.0.0",
    "socket.io": "^1.1.0",
    "underscore.assert": "^1.1.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.2",
    "babel-loader": "^5.3.2",
    "eslint": "^1.4.3",
    "eslint-plugin-react": "^3.3.2",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.11.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.1",
    "gulp-jslint": "^0.2.2",
    "gulp-mocha": "^2.2.0",
    "istanbul": "^0.4.5",
    "jest": "^17.0.0",
    "mocha": "^2.3.4"
  }
}
